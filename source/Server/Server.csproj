    <PackageReference Include="Octopus.Data" Version="5.3.1-enh-tinytype-ids.6" />
    <PackageReference Include="Octopus.Diagnostics" Version="1.3.5" />
    <PackageReference Include="Octopus.Time" Version="1.1.6" />
